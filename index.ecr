                        <div class="colorlib-narrow-content" style="max-height: -webkit-fill-available; display: flex; flex-direction: column;">
                            <div class="row" style="flex: 1;">
                                <div class="col-md-6 col-md-offset-3 col-md-pull-3 animate-box" data-animate-effect="fadeInLeft">
                                    <span class="heading-meta">Here's all your tickets</span>
                                    <a href="#" ><h2 class="colorlib-heading">All Ticket</h2></a>
                                </div>
                            </div>
                            <div class="row" style="overflow: hidden; overflow-y: scroll; display: block; position: relative; flex: 1;">
                                <div class="col-md-12">
                                    <div v-show="tickets.length !== 0" class="timeline-centered">
                                        <article v-for="ticket in tickets" class="timeline-entry animate-box" style="opacity: 1;">
                                            <div class="timeline-entry-inner">
                                                <div v-if="ticket.state === 0" class="timeline-icon color-3"><i class="icon-pen2"></i></div>
                                                <div v-if="ticket.state === 3" class="timeline-icon color-2"><i class="icon-pen2"></i></div>
                                                <div v-if="ticket.state === 1" class="timeline-icon color-5"><i class="icon-pen2"></i></div>
                                                <div v-if="ticket.state === 2" class="timeline-icon color-4"><i class="icon-pen2"></i></div>

                                                <div class="timeline-label" style="display: flex;">
                                                    <img :src="'/ticket/' + ticket.id + '/img'" style="max-height: 100px; margin-right: 15px;">
                                                    <p style="flex: 1;">
                                                        <span>Ticket n# {{ticket.id}}</span> <span>created at {{ new Date(ticket.created_at).toLocaleString() }}</span><br>
                                                        <span>Intervention date : {{ new Date(ticket.intervention_time).toLocaleString() }}</span><br>
                                                        <span>Building : {{ticket.building.address}}</span><br>
                                                        <span>Floor : {{ticket.floor}}</span> - <span>Orientation : {{ticket.orientation}}°</span>
                                                    </p>
                                                    <p class="icon" style="display: flex; flex-direction: column; margin-top: 0;">
                                                        <span v-show="ticket.state != 3 && ticket.state != 1 && ticket.state != 2"><a style="cursor: pointer;" @click="set_state_ticket(ticket.id, 'refuse')"><i class="icon-share3"></i>Refuse</a></span>
                                                        <span v-show="ticket.state != 3 && ticket.state != 1 && ticket.state != 2"><a style="cursor: pointer;" @click="set_state_ticket(ticket.id, 'accept')"><i class="icon-eye"></i>Accept</a></span>
                                                        <span v-show="ticket.state != 3 && ticket.state != 2 && ticket.state != 0"><a style="cursor: pointer;" @click="set_state_ticket(ticket.id, 'done')"><i class="icon-eye"></i>Done</a></span>
                                                    </p>
                                                </div>
                                            </div>
                                        </article>
                                        <article class="timeline-entry begin animate-box" style="opacity: 1;">
                                            <div class="timeline-entry-inner">
                                                <div class="timeline-icon color-none">
                                                </div>
                                            </div>
                                        </article>
                                    </div>
                                    <div v-show="tickets.length === 0">
                                        No tickets
                                    </div>
                                </div>
                            </div>
                            <div style="height: 9em; flex: 1;">
                            </div>
                        </div>
                    </section>
