<%- if logged && !is_admin -%>
<!-- Ticket modal -->
<div id="modal_ticket" class="modal">
    <div class="modal-dialog animated">
        <div class="modal-content">
                <div class="modal-header" style="text-align: center;">
                    <strong>New Ticket</strong>
                </div>

                <form>
                    <div class="form-row" style="display: flex;">
                        <div class="form-group col-md-6">
                            <label>Building
                                <span style="color: orange;" v-show="!$v.ticket.building.required && $v.$dirty">Required </span>
                            </label>
                            <select class="form-control" v-model="ticket.building">
                                <option selected disabled>Choose...</option>
                                <option v-for="building in buildings" :value="building.id">{{building.address}}</option>
                            </select>
                        </div>
                        <div class="form-group col-md-6">
                            <label>Orientation
                                <span style="color: orange;" v-show="!$v.ticket.orientation.required && $v.$dirty">Required </span>
                            </label>
                            <select class="form-control" v-model="ticket.orientation">
                                <option selected disabled>Choose...</option>
                                <option value="0">North</option>
                                <option value="45">North East</option>
                                <option value="90">East</option>
                                <option value="135">South East</option>
                                <option value="180">South</option>
                                <option value="225">South West</option>
                                <option value="270">West</option>
                                <option value="315">North West</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-row" style="display: flex;">
                        <div class="form-group col-md-6">
                            <label>Floor
                                <span style="color: orange;" v-show="!$v.ticket.building_floor.required && $v.$dirty">Required </span>
                            </label>
                            <input type="number" class="form-control" placeholder="zzz" v-model="ticket.building_floor">
                        </div>
                        <div class="form-group col-md-6">
                            <label>Time
                                <span style="color: orange;" v-show="!$v.ticket.intervention_date.required && $v.$dirty">Required </span>
                            </label>
                            <input type="datetime-local" class="form-control" placeholder="zzz" v-model="ticket.intervention_date">
                        </div>
                    </div>
                    <div class="form-row" style="display: flex;">
                        <div class="form-group col-md-4">
                            <label>Picture
                            </label>
                            <input type="file" class="form-control" id="file" ref="file" @change="handle_ticket_file_upload($event)"/>
                        </div>
                    </div>
                </form>

                <div class="modal-footer">
                    <a class="btn btn-primary" style="margin-bottom: 0;"v-on:click="send_ticket">Create</a>
                    <a class="btn btn-primary" style="margin-bottom: 0;" onclick="modal_ticket.close();">Close</a>
                </div>
        </div>
    </div>
</div>


<!-- Building modal -->
<div id="modal_building" class="modal">
    <div class="modal-dialog animated">
        <div class="modal-content">
                <div class="modal-header" style="text-align: center;">
                    <strong>New Building</strong>
                </div>
                <form style="">
                    <div class="form-row" style="display: flex;">
                        <div class="form-group col-md-6">
                            <label>Number of floors
                                <span style="color: orange;" v-show="!$v.building.nb_floor.required && $v.$dirty">Required </span>
                            </label>
                            <input type="text" class="form-control" placeholder="Number of floors" v-model="building.nb_floor">
                        </div>
                        <div class="form-group col-md-12">
                            <label>Address
                                <span style="color: orange;" v-show="!$v.building.address.required && $v.$dirty">Required </span>
                            </label>
                            <input type="text" class="form-control" placeholder="1234 Main St" v-model="building.address">
                        </div>
                    </div>
                    <div class="form-row" style="display: flex;">
                        <div class="form-group col-md-4">
                            <label>Country
                                <span style="color: orange;" v-show="!$v.building.country.required && $v.$dirty">Required </span>
                            </label>
                            <input type="text" class="form-control" placeholder="France" v-model="building.country">
                        </div>
                        <div class="form-group col-md-4">
                            <label>Picture
                            </label>
                            <input type="file" class="form-control" id="file" ref="file" @change="handle_building_file_upload($event)"/>
                        </div>
                    </div>
                </form>
                <div class="modal-footer">
                    <a class="btn btn-primary" style="margin-bottom: 0;" v-on:click="send_building">Create</a>
                    <a class="btn btn-primary" style="margin-bottom: 0;" onclick="modal_building.close();">Close</a>
                </div>
        </div>
    </div>
</div>
<%- end -%>

            </div><!-- end:container-wrap -->
        </div><!-- end:colorlib-page -->
        <%- if logged && is_admin -%>
        <script src="js/admin.js"></script>
        <%- else logged && !is_admin -%>
        <script src="js/client.js"></script>
        <script type="text/javascript" src="js/rmodal.min.js"></script>
        <script type="text/javascript">
            window.onload = function() {
                let modal_ticket = new RModal(document.getElementById('modal_ticket'));

                document.addEventListener('keydown', function(ev) {
                    modal_ticket.keydown(ev);
                }, false);

                document.getElementById('show_modal_ticket')
                .addEventListener("click", function(ev) {
                    ev.preventDefault();
                    modal_ticket.open();
                }, false);

                window.modal_ticket = modal_ticket;


                let modal_building = new RModal(document.getElementById('modal_building'));

                document.addEventListener('keydown', function(ev) {
                    modal_building.keydown(ev);
                }, false);

                document.getElementById('show_modal_building')
                .addEventListener("click", function(ev) {
                    ev.preventDefault();
                    modal_building.open();
                }, false);

                window.modal_building = modal_building;
            }
        </script>
        <%- end -%>
        <!-- jQuery -->
        <script src="js/jquery.min.js"></script>
        <!-- jQuery Easing -->
        <script src="js/jquery.easing.1.3.js"></script>
        <!-- Bootstrap -->
        <script src="js/bootstrap.min.js"></script>
        <!-- Waypoints -->
        <script src="js/jquery.waypoints.min.js"></script>
        <!-- Flexslider -->
        <script src="js/jquery.flexslider-min.js"></script>
        <!-- Owl carousel -->
        <script src="js/owl.carousel.min.js"></script>
        <!-- Counters -->
        <script src="js/jquery.countTo.js"></script>
        <!-- MAIN JS -->
        <script src="js/main.js"></script>
    </body>
</html>
